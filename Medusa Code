#include <Servo.h>

Servo servo1; //medusa turning servo
Servo servo2; //poseidon servo
Servo servo3; //athena servo
Servo servo4; //soldiers servo
const int buttonPin1 = 2; //light sensor for poseidon? 
const int buttonPin2 = 1; //something 
//how to set up led again? 
void setup() {
  servo1.attach(9); // Servo goes on pin 9
  servo2.attach(10);
  servo3.attach(11);
  servo4.attach(12);
  pinMode(buttonPin1, INPUT);
  pinMode(buttonPin2, INPUT);
}

void loop(){
  servo1.
  if(digitalRead(buttonPin) == HIGH){
    servo2.write(180); //What angle are we turning it? 0-180
  }else{
    servo3.write(0);
  }
}
